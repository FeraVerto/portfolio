{
  "settings": {
    "react": {
      "version": "latest"
    },
    "import/resolver": {
      "node": {
        "paths": ["src"],
        "extensions": [".js", ".jsx"]
      }
    }
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "plugins": ["react", "prettier"],
  "extends": ["react-app", "prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "endOfLine": "auto"
      }
    ]
  }
}